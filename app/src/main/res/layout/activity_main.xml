<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bienvenida"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D5C6D8"
    android:textAlignment="center"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/textView"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:background="#ff0065"
        android:fontFamily="@font/lato_bold"
        android:lineSpacingExtra="8sp"
        android:text="Inscripción de usuarios"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:typeface="normal" />

    <EditText
        android:id="@+id/etxtCedula"
        android:layout_width="272dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="232dp"
        android:layout_marginEnd="33dp"
        android:ems="10"
        android:fontFamily="@font/lato"
        android:hint="Ingreso de cédula"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/txtCedula"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="246dp"
        android:fontFamily="@font/lato_bold"
        android:text="Cédula"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="normal" />

    <TextView
        android:id="@+id/txtNombres"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="298dp"
        android:fontFamily="@font/lato_bold"
        android:text="Nombres"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="normal" />

    <EditText
        android:id="@+id/etxtNombres"
        android:layout_width="272dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="287dp"
        android:layout_marginEnd="31dp"
        android:ems="10"
        android:fontFamily="@font/lato"
        android:hint="Ingreso de nombres"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/txtFechaNac"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="24dp"
        android:layout_marginTop="350dp"
        android:fontFamily="@font/lato_bold"
        android:text="Fecha Nac."
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="normal" />

    <TextView
        android:id="@+id/txtCiudad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="402dp"
        android:fontFamily="@font/lato_bold"
        android:text="Ciudad"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="normal" />

    <EditText
        android:id="@+id/etxtCiudad"
        android:layout_width="272dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="392dp"
        android:layout_marginEnd="32dp"
        android:ems="10"
        android:fontFamily="@font/lato"
        android:hint="Ingreso de Ciudad"
        android:inputType="text"
        android:textAlignment="viewStart"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/txtGenero"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="460dp"
        android:fontFamily="@font/lato_bold"
        android:text="Género"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="normal" />

    <RadioButton
        android:id="@+id/bt1Masculino"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="113dp"
        android:layout_marginTop="450dp"
        android:fontFamily="@font/lato"
        android:text="Masculino"
        android:textSize="16sp" />

    <RadioButton
        android:id="@+id/bt1Femenino"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="234dp"
        android:layout_marginTop="450dp"
        android:fontFamily="@font/lato"
        android:text="Femenino"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/txtCorreo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerVertical="true"
        android:layout_marginStart="26dp"
        android:layout_marginTop="517dp"
        android:fontFamily="@font/lato_bold"
        android:text="Correo"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="normal" />

    <EditText
        android:id="@+id/emtxt1Correo"
        android:layout_width="274dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="503dp"
        android:layout_marginEnd="30dp"
        android:ems="10"
        android:fontFamily="@font/lato"
        android:hint="Ingreso de correo electrónico"
        android:inputType="textEmailAddress"
        android:textAlignment="viewStart"
        android:textSize="16sp" />

    <TextView
        android:id="@+id/txtTelefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="25dp"
        android:layout_marginTop="572dp"
        android:fontFamily="@font/lato_bold"
        android:text="Teléfono"
        android:textSize="16sp"
        android:textStyle="bold"
        android:typeface="normal" />

    <EditText
        android:id="@+id/ptxt1Telf"
        android:layout_width="270dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="227dp"
        android:ems="10"
        android:fontFamily="@font/lato"
        android:hint="Ingreso de teléfono"
        android:inputType="phone"
        android:textAlignment="viewStart"
        android:textSize="16sp" />

    <Button
        android:id="@+id/bt2Enviar"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="210dp"
        android:layout_marginBottom="106dp"
        android:background="#ff0065"
        android:backgroundTint="#ff0065"
        android:fontFamily="@font/lato_bold"
        android:onClick="registrarDatos"
        android:text="Enviar"
        android:textAlignment="center"
        android:textSize="16sp"
        tools:text="Enviar" />

    <Button
        android:id="@+id/bt1Limpiar"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="106dp"
        android:background="#ff0065"
        android:backgroundTint="#ff0065"
        android:fontFamily="@font/lato_bold"
        android:onClick="limpiarCampos"
        android:text="Limpiar"
        android:textAlignment="center"
        android:textSize="16sp"
        tools:text="Limpiar" />

    <EditText
        android:id="@+id/etd1FechaNac"
        android:layout_width="272dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="334dp"
        android:layout_marginEnd="32dp"
        android:ems="10"
        android:fontFamily="@font/lato"
        android:hint="Ingresar fecha de nacimiento"
        android:inputType="date"
        android:textAlignment="viewStart"
        android:textSize="16sp" />

    <ImageView
        android:id="@+id/iv1Imagen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="66dp"
        tools:srcCompat="@tools:sample/avatars" />

</RelativeLayout>